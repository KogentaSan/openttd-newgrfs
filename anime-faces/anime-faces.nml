grf {
    grfid: "ZAM\1";
    name: string(STR_GRF_NAME);
    desc: string(STR_GRF_DESCRIPTION);
    url:  string(STR_GRF_URL);
    version: 1;
    min_compatible_version: 1;
}

template template_manager_face(f, z) {
    [0, 0, 92*z, 112*z, 0, 0, CROP, f]
}
template template_manager_face_mask(f, m, z) {
    [0, 0, 92*z, 112*z, 0, 0, CROP, f, m]
}
template blank_8bpp() {
    [0, 0, 1, 1, 0, 0, NOCROP, "blankblue_8bpp.png"]
}
template blank_32bpp() {
    [0, 0, 1, 1, 0, 0, NOCROP, "blank_32bpp.png"]
}

